{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.2.2"
  },
  "scripts": {
    "rollupwatch": "node_modules/.bin/rollup -w -m -c themes/wiki-i18n/source/js/build/config.js",
    "jsdev": "SET NODE_ENV=dev&& npm run rollupwatch",
    "jsbuild": "node themes/wiki-i18n/source/js/build/build.js",
    "start": "SET NODE_ENV=dev&& hexo clean && hexo generate && hexo serve --watch",
    "deploy": "npm run jsbuild && npm run test && hexo clean && hexo generate && hexo deploy",
    "lint": "eslint . && echo 'ESLint Passed!\\n'",
    "test": "npm run lint"
  },
  "dependencies": {
    "hexo": "^3.2.0",
    "hexo-deployer-git": "^0.2.0",
    "hexo-front-matter-excerpt": "^0.2.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-archive-alphabet-i18n": "^0.1.0",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-index-i18n": "^0.1.1",
    "hexo-generator-json-content": "^2.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-media-marked": "^0.1.8",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-sass": "^0.2.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0",
    "hexo-tag-bootstrap": "0.0.8",
    "yaml-front-matter": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-watch": "^2.5.0",
    "uglify-js": "^2.7.3"
  }
}
