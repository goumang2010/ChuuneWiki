{
  "extends": "airbnb-base/legacy",
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "no-var": 0,
    "one-var": 0,
    "vars-on-top": 0,
    "no-use-before-define": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": 0,
    "no-param-reassign": 0,
    "func-names": 0
  }
}
